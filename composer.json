{
    "name": "grizmio/spatialdatatypes",
    "description": "Grizmio/SpatialDataTypes plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Grizmio\\SpatialDataTypes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grizmio\\SpatialDataTypes\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
